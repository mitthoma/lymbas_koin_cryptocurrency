nerated":{"line":113,"column":22},"source":"components/TransactionPool.js","original":{"line":62,"column":17}},{"generated":{"line":114,"column":0},"source":"components/TransactionPool.js","original":{"line":60,"column":3}},{"generated":{"line":114,"column":29},"source":"components/TransactionPool.js","original":{"line":46,"column":3}},{"generated":{"line":114,"column":30},"source":"components/TransactionPool.js","original":{"line":45,"column":2}},{"generated":{"line":115,"column":0},"source":"components/TransactionPool.js","original":{"line":69,"column":2}},{"name":"Component","generated":{"line":119,"column":2},"source":"components/TransactionPool.js","original":{"line":9,"column":30}},{"name":"TransactionPool","generated":{"line":121,"column":15},"source":"components/TransactionPool.js","original":{"line":72,"column":15}}],"sources":{"components/TransactionPool.js":"import React, { Component } from 'react';\nimport Transaction from './Transaction';\nimport { Button } from 'react-bootstrap';\nimport { Link } from 'react-router-dom';\nimport history from '../history';\n\nconst POLL_INTERVAL_MS = 10000;\n\nclass TransactionPool extends Component {\n\n\tstate = { transactionPoolMap: {} }\n\n\tfetchTransactionPoolMap = () => {\n\t\tfetch(`${document.location.origin}/api/transaction-pool-map`)\n\t\t.then(response => response.json())\n\t\t.then(json => this.setState({ transactionPoolMap: json }));\n\t}\n\n\tfetchMineTransactions = () => {\n\t\tfetch(`${document.location.origin}/api/mine-transactions`)\n\t\t.then(response => {\n\t\t\tif (response.status === 200) {\n\t\t\t\talert('success');\n\t\t\t\thistory.push('/blocks');\n\t\t\t} else {\n\t\t\t\talert('The mine-transactions block request did not complete');\n\t\t\t}\n\t\t});\n\t}\n\n\tcomponentDidMount() {\n\t\tthis.fetchTransactionPoolMap();\n\n\t\tthis.fetchPoolMapInterval = setInterval(\n\t\t\t()\t=> this.fetchTransactionPoolMap(),\n\t\t\tPOLL_INTERVAL_MS\n\t\t);\n\t}\n\n\tcomponentWillUnmount() {\n\t\tclearInterval(this.fetchPoolMapInterval);\n\t}\n\n\trender() {\n\t\treturn(\n\t\t\t<div className='TransactionPool'>\n\t\t\t<div><Link to='/'>Home</Link></div>\n\t\t\t<h3>Transaction Pool</h3>\n\t\t\t{\n\t\t\t\tObject.values(this.state.transactionPoolMap).map(transaction => {\n\t\t\t\t\treturn (\n\t\t\t\t\t\t<div key={transaction.id}>\n\t\t\t\t\t\t\t<hr />\n\t\t\t\t\t\t\t<Transaction transaction={transaction} />\n\t\t\t\t\t\t</div>\n\t\t\t\t\t)\n\t\t\t\t})\n\t\t\t}\n\t\t\t<hr />\n\t\t\t<Button\n\t\t\tbsStyle=\"danger\"\n\t\t\tonClick={this.fetchMineTransactions}\n\t\t\t>\n\t\t\t\tMine Transactions\n\n\t\t\t</Button>\n\t\t\t</div>\n\t\t)\n\t}\n}\n\nexport default TransactionPool;\n\n"},"lineCount":null}},"error":null,"hash":"22a1dee92d8ea7a1238997b46a55c7ed","cacheData":{"env":{}}}